  <template name="fore" value="for (var $INC$ in $COL$) {&#10;    if ($COL$.hasOwnProperty($INC$)) {&#10;        var $VAR$ = $COL$[$INC$];&#10;        $END$&#10;    }&#10;}" description="simple foreach" toReformat="false" toShortenFQNames="true">
